{
  "name": "@westpac/ui",
  "version": "0.6.0",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/*/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./css": {
      "default": "./dist/css/westpac-ui.min.css"
    },
    "./icon": {
      "default": "./dist/components/icon/index.js"
    },
    "./hook": {
      "default": "./dist/hook/index.js"
    },
    "./pictogram": {
      "default": "./dist/components/pictogram/index.js"
    },
    "./symbol": {
      "default": "./dist/components/symbol/index.js"
    },
    "./tailwind": {
      "default": "./dist/tailwind/index.js"
    }
  },
  "files": [
    "*",
    "!coverage",
    "!stories",
    "!vitest.setup.ts",
    "!vitest.config.ts",
    "!**/*.{test,spec}.{ts,tsx,js,jsx}"
  ],
  "scripts": {
    "dev": "pnpm build:watch",
    "clean": "shx rm -rf dist",
    "build:css:min": "shx mkdir -p dist/css && cross-env NPM_ENV=production postcss src/css/global.css  > dist/css/westpac-ui.min.css",
    "build:css": "shx mkdir -p dist/css && cross-env NPM_ENV=development postcss src/css/global.css  > dist/css/westpac-ui.css",
    "build:css:all": "pnpm build:css && pnpm build:css:min",
    "build:types": "tsc --project tsconfig.build.json",
    "build:esm": "swc ./src -d dist --no-swcrc --config-file .swcrc",
    "build:icons": "ts-node --esm ./utils/build-icons/index.ts",
    "build:watch": "pnpm clean && pnpm build:types && pnpm build:esm -w && pnpm build:css:all",
    "build": "pnpm clean && pnpm build:types && pnpm build:esm && pnpm build:css:all",
    "check-types": "tsc --noemit --emitDeclarationOnly false",
    "lint": "eslint \"./src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx,js,jsx}\" --fix",
    "test": "vitest run",
    "test:watch": "vitest --no-coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:component": "plop"
  },
  "devDependencies": {
    "@react-types/accordion": "3.0.0-alpha.14",
    "@react-types/combobox": "~3.7.0",
    "@react-types/shared": "~3.18.1",
    "@storybook/addon-a11y": "~7.4.3",
    "@storybook/addon-actions": "~7.4.3",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/addon-styling": "~1.3.2",
    "@storybook/blocks": "^7.0.21",
    "@storybook/manager-api": "~7.4.6",
    "@storybook/react": "^7.0.21",
    "@storybook/react-vite": "^7.0.21",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "~7.4.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.35",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/lodash-es": "~4.17.8",
    "@types/prettier": "~2.7.3",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "@vitest/coverage-c8": "^0.31.0",
    "chokidar": "^3.5.3",
    "colorjs.io": "~0.4.5",
    "cross-env": "~7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-storybook": "~0.6.12",
    "glob": "~10.3.3",
    "jsdom": "^22.0.0",
    "plop": "~3.1.2",
    "postcss": "~8.4.24",
    "postcss-cli": "~10.1.0",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "storybook": "^7.0.21",
    "tailwind-variants": "~0.1.13",
    "tailwindcss": "~3.3.2",
    "tailwindcss-themer": "~3.1.0",
    "ts-node": "~10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@duetds/date-picker": "~1.4.0",
    "@react-aria/accordion": "3.0.0-alpha.19",
    "@react-aria/utils": "~3.18.0",
    "@react-spectrum/utils": "~3.10.0",
    "@react-types/tabs": "~3.3.0",
    "@westpac/eslint-config": "workspace:~",
    "@westpac/test-config": "workspace:~",
    "@westpac/ts-config": "workspace:~",
    "clsx": "^1.2.1",
    "framer-motion": "~10.12.16",
    "react-aria": "~3.25.0",
    "react-stately": "~3.23.0"
  },
  "overrides": {
    "react-aria": {
      "@types/react": "$@types/react"
    }
  },
  "peerDependencies": {
    "@duetds/date-picker": "~1.4.0",
    "colorjs.io": "~0.4.5",
    "react": ">=18.2.0",
    "tailwind-variants": "~0.1.13",
    "tailwindcss": "~3.3.2",
    "tailwindcss-themer": "~3.1.0"
  }
}
